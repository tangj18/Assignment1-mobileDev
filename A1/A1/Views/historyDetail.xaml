<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="A1.Views.historyDetail"
             x:Name="title">
    <ContentPage.Content>
        <StackLayout VerticalOptions="Center" HorizontalOptions="Center">
            <Label x:Name="itemName" TextColor="Black"/>
            <Label x:Name="itemQuantity"
                   TextColor="Black" />
            <Label x:Name="itemTime"
                   TextColor="Black" />
            <Label x:Name="itemTotal"
                   TextColor="Black" />
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>